<Window x:Class="CuratorsHelper.View.StudentsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CuratorsHelper.View"
        xmlns:helpers="clr-namespace:CuratorsHelper"
        mc:Ignorable="d"
       Title="CuratorsHelper" 
        Height="900"
        Width="1440" 
        WindowState="Maximized" 
        Background="#E7E7E7"
        WindowStyle="None"
        ResizeMode="NoResize"
        Closed="Window_Closed">
    <Window.ContextMenu>
        <ContextMenu>
            <MenuItem Header="Печать"
                      Click="MenuItem_Click"/>
        </ContextMenu>
    </Window.ContextMenu>
    <Window.Resources>
        <helpers:ConverterImage x:Key="ConverterImage"/>
        <Image x:Key="closeApp" Source="/images/close.png"/>
        <Style TargetType="Button" x:Key="menuStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border 
                                    x:Name="border" 
                                    CornerRadius="50"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    ClipToBounds="True" 
                                    BorderThickness="{TemplateBinding BorderThickness}" 
                                    Background="{TemplateBinding Background}" 
                                    SnapsToDevicePixels="true">

                            <ContentPresenter 
                                        x:Name="contentPresenter" 
                                        Focusable="False" 
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        Margin="{TemplateBinding Padding}" 
                                        RecognizesAccessKey="True"
                                        SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>

                        </Border>

                        <ControlTemplate.Triggers>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="BorderThickness" TargetName="border" Value="0.5"/>
                                <Setter Property="Margin" TargetName="border" Value="0.5"/>
                                <Setter Property="BorderBrush" 
                                                TargetName="border" 
                                                Value="#CCAD0000"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" TargetName="border" Value="#FF1B1B1B"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="#CCAD0000"/>
                            </Trigger>
                        </ControlTemplate.Triggers>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </Window.Resources>
    
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.039*"/>
            <RowDefinition Height="0.12*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Background="#7196B9" Grid.Row="0"/>

        <Grid Background="#222" Grid.Row="0"/>

        <TextBlock
            Text="CuratorsHelper"
            HorizontalAlignment="Left"
            Foreground="White"
            VerticalAlignment="Center"
            FontSize="20"
            Margin="10 0 0 0"/>

        <StackPanel Grid.Row="0" Orientation="Horizontal"
                    HorizontalAlignment="Right">


            <Button  
                Click="collapseWindow_Click"
            Name="collapseWindow"
            Content="___"
            Foreground="#888888"  
            Margin="0 0 5 0"
            Width="20" Height="20"
            HorizontalAlignment="Right"
            Background="#222"
            FontWeight="Bold"
            BorderThickness="0"
            Style="{StaticResource menuStyle}">

            </Button>


            <Button 
                Click="closeWindow_Click"
            Name="closeWindow"
            Content="╳"
            Foreground="#888888"  
            Width="20" Height="20"
            Background="#222"
            HorizontalAlignment="Right"
            BorderThickness="0"
                Style="{StaticResource menuStyle}">

            </Button>


        </StackPanel>


        <Grid Background="#CDCDCD" Grid.Row="1"/>

        <Image
            Grid.Row="1"
            HorizontalAlignment="Left"
            Width="85"
            Margin="15 0 0 0"
            Source="/images/BackBtn.png">
            <Image.Style>
                <Style TargetType="Image">
                    <EventSetter Event="MouseDown" Handler="Image_MouseDown"/>
                </Style>
            </Image.Style>
        </Image>

        <StackPanel Grid.Row="1"
                    HorizontalAlignment="Right"
                    Margin="0 0 10 0">
            <StackPanel.Style>
                <Style TargetType="StackPanel">
                    <EventSetter Event="MouseDown" Handler="StackPanel_MouseDown"/>
                </Style>
            </StackPanel.Style>

            <Image
                Width="60"
                Source="/images/bxs_exit.png"/>

            <TextBlock
                Text="Выход"
                Foreground="#222"
                FontSize="28"
                FontFamily="Inter"/>

        </StackPanel>

        <Grid Grid.Row="2">

            <Grid.RowDefinitions>
                <RowDefinition Height="0.1*"/>
                <RowDefinition Height="0.1*"/>
                <RowDefinition Height="0.1*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Viewbox
                  Grid.Row="0"
                  Grid.RowSpan="3"
                  HorizontalAlignment="Left"
                  Stretch="Uniform"
                  Width="150"
                Margin="50 7 0 0">
                <Image Name="StudentPhoto"
                       MouseDown="StudentPhoto_MouseDown">
                    <Image.Source>
                        <Binding Path="photo">
                            <Binding.Converter>
                                <helpers:ConverterImage/>
                            </Binding.Converter>
                        </Binding>
                    </Image.Source>
                </Image>
            </Viewbox>

            <Image
                HorizontalAlignment="Right"
                Name="DeleteStudentImage"
                Margin="10 10 30 10">
                <Image.Style>
                    <Style TargetType="Image">
                        <EventSetter Event="MouseDown" Handler="Image_MouseDown_6"/>
                        <Setter Property="Source"
                                    Value="/images/deleteicon.png"/>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Source" 
                                            Value="/images/deleteicon_hover.png"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Image.Style>
            </Image>

            <StackPanel
                Grid.Row="2"
                HorizontalAlignment="Left"
                Margin="250 0 0 0"
                VerticalAlignment="Center"
                Orientation="Horizontal">

                <Image 
                Name="IMGAccept"
                Visibility="Hidden"
                Height="30"
                Width="30">
                    <Image.Style>
                        <Style TargetType="Image">
                            <EventSetter Event="MouseDown" Handler="Image_MouseDown_4"/>
                            <Setter Property="Source"
                                    Value="/images/accept_ico.png"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Source" 
                                            Value="/images/accept_ico_WHITE.png"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>
                </Image>

                <Image 
                Name="IMGCancel"
                Visibility="Hidden"
                Height="40"
                Width="40"
                Margin="10 0 0 0">
                    <Image.Style>
                        <Style TargetType="Image">
                            <EventSetter Event="MouseDown" Handler="Image_MouseDown_5"/>
                            <Setter Property="Source"
                                    Value="/images/cancel_ico.png"/>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Source" 
                                            Value="/images/cancel_ico_WHITE.png"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>
                </Image>
                
            </StackPanel>
           


            <TextBlock
                FontFamily="Times New Roman"
                Grid.Row="0"
                HorizontalAlignment="Center" 
                VerticalAlignment="Center"
                Text="Карта персонофицированного учета"
                FontSize="36"/>

            <ComboBox
                SelectionChanged="studentsCombo_SelectionChanged"
                Name="studentsCombo"
                Style="{DynamicResource ComboBoxStyle1}"
                BorderThickness="0 0 0 2"
                Grid.Row="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                VerticalContentAlignment="Bottom"
                HorizontalContentAlignment="Center"
                MinWidth="350"
                MinHeight="50"
                FontSize="22"
                DisplayMemberPath="FIO"
                SelectedValuePath="id_student"/>


            <Grid Grid.Row="3" 
                  Margin="70 0 0 0"
                  Name="MainGrid">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="0.07*"/>
                    <ColumnDefinition Width="0.14*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="0.07*"/>
                    <ColumnDefinition Width="0.07*"/>
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition Height="3.2*"/>
