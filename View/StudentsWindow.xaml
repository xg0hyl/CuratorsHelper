                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    FontSize="30"
                    Text="ФИО"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                <TextBox
                    IsReadOnly="True"
                    Grid.Row="1"
                    Grid.Column="0"
                    Name="FioText"
                    Text="{Binding FIO}"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                    VerticalContentAlignment="Center"
                    />

                <TextBlock
                    VerticalAlignment="Center"
                    Grid.Row="2"
                    Grid.Column="0"
                    FontSize="30"
                    Text="Дата рождения"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                <DatePicker
                    Grid.Row="3"
                    Grid.Column="0"
                    Name="DateBornDate"
                    SelectedDateFormat="Long"
                    Text="{Binding date_born, StringFormat=yyyy.MM.dd}"
                    Style="{DynamicResource DatePickerStyle2}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch">
                </DatePicker>


                <TextBlock
                    VerticalAlignment="Center"
                    Grid.Row="4"
                    Grid.Column="0"
                    FontSize="30"
                    Text="Гражданство"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                <!--УКАЗАТЬ ГРАЖДАНСТВО-->
                
                <TextBox
                    IsReadOnly="True"
                    Grid.Row="5"
                    Grid.Column="0"
                    Name="NationalText"
