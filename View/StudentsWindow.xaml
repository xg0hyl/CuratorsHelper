                    Text="{Binding room}"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                    PreviewTextInput="HostelText_PreviewTextInput"/>






                <TextBlock
                    Grid.Row="0"
                    Grid.Column="3"
                    FontSize="30"
                    Text="Состояние здоровья"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                <TextBox
                    IsReadOnly="True"
                    Grid.Row="1"
                    Grid.Column="3"
                    Name="HealthText"
                    Text="{Binding health}"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                    />

                <TextBlock
                    Grid.Row="2"
                    Grid.Column="3"
                    FontSize="30"
                    Text="Сведения о семье"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                <!--ПРОПИСАТЬ СЕМЬЮ-->
                
                <TextBox
                    IsReadOnly="True"
                    Grid.Row="3"
                    Grid.Column="3"
                    Name="MotherText"
                    AcceptsReturn="True"
                    TextWrapping="Wrap"
                    Grid.RowSpan="2"
                    Text=""
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="18"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                    VerticalContentAlignment="Bottom"
                    />

                <TextBox
                    IsReadOnly="True"
                    Grid.Row="5"
                    Grid.Column="3"
                    Name="FatherText"
                    AcceptsReturn="True"
                    TextWrapping="Wrap"
                    Grid.RowSpan="2"
                    Text=""
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="18"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                     VerticalContentAlignment="Bottom"
                    />

                <TextBlock
                    Name="OtherTextBlock"
                    VerticalAlignment="Center"
                    Grid.Row="7"
                    Grid.Column="3"
                    FontSize="30"
                    Text="Другие члены семьи"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                
                <!--ПОД ВОПРОСОМ КОМБОБОКСА-->
                
                <TextBox
                    IsReadOnly="True"
                    Grid.Row="8"
                    Grid.Column="3"
                    Name="Brothers"
                    Text=""
                    AcceptsReturn="True"
                    TextWrapping="Wrap"
                    Grid.RowSpan="2"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="18"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                     VerticalContentAlignment="Bottom"
                    />

                <TextBox
                    IsReadOnly="True"
                    Grid.Row="10"
                    Grid.Column="3"
                    Name="Sisters"
                    Text=""
                    AcceptsReturn="True"
                    TextWrapping="Wrap"
                    Grid.RowSpan="2"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="18"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                     VerticalContentAlignment="Bottom"
                    />

                <TextBlock
                    Grid.Row="12"
                    Grid.Column="3"
                    FontSize="30"
                    Text="Отношение с семьей"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    Name="RelationshipTextBlock"
                    HorizontalAlignment="Center"/>

                <TextBox
                    IsReadOnly="True"
                    Grid.Row="13"
                    Grid.Column="3"
                    Name="RelationshipText"
                    Text="{Binding relationship}"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"
                    />

                <TextBlock
                    Name="HobbiesTextBlock"
                    Grid.Row="14"
                    Grid.Column="3"
                    FontSize="30"
                    Text="Увлечения"
                    FontFamily="Times New Roman"
                    FontWeight="Bold"
                    HorizontalAlignment="Center"/>

                
                <!--ПРОПИСАТЬ ХОББИ-->
                
                <TextBox
                    IsReadOnly="True"
                    Grid.Row="15"
                    Grid.Column="3"
                    Name="HobbiesText"
                    Text="{Binding hobby}"
                    Style="{DynamicResource TextBoxStyle1}"
                    FontSize="24"
                    FontFamily="Times New Roman"
                    HorizontalAlignment="Stretch"/>


                
                
